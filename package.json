{
  "name": "@dbetka/vue-material-icons",
  "version": "0.2.6",
  "description": "Vue Material Design Icons",
  "keywords": [
    "material",
    "design",
    "icons",
    "vue",
    "dbetka",
    "@dbetka",
    "google"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "download-icons": "node bin/create-list-of-icons.js",
    "download-css-and-fonts": "node bin/download-css-and-fonts.js",
    "download-all": "npm run download-icons && npm run download-css-and-fonts",
    "dev": "webpack serve --config webpack.dev.js",
    "build": "npx bili && npx bili --input ./src/local.js",
    "download-and-build": "npm run download-all && npm run build",
    "push-to-whatusea": "bash ./bin/push-to-whatusea.sh",
    "download-build-push": "npm run download-and-build && npm run push-to-whatusea"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbetka/vue-material-icons.git"
  },
  "author": "dbetka",
  "bugs": {
    "url": "https://github.com/dbetka/vue-material-icons/issues"
  },
  "homepage": "https://github.com/dbetka/vue-material-icons#readme",
  "dependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@dbetka/utils": "^0.1.3",
    "@vue/cli-plugin-babel": "^4.5.9",
    "async": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "bili": "^4.9.0",
    "convert-units": "^2.3.4",
    "core-js": "^3.10.0",
    "css-loader": "^3.4.2",
    "download": "^8.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.2.0",
    "node-fetch": "^2.6.1",
    "replace-in-file": "^6.2.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^7.2.0",
    "vue-loader": "^15.9.5",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.1"
  }
}
