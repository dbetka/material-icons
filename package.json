{
  "name": "@dbetka/vue-material-icons",
  "version": "0.3.0",
  "description": "Vue3 Material Design Icons",
  "license": "MIT",
  "author": "dbetka",
  "keywords": [
    "material",
    "design",
    "icons",
    "vue",
    "vue3",
    "dbetka",
    "@dbetka",
    "dbetka",
    "google"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dbetka/vue-material-icons"
  },
  "scripts": {
    "download-icons": "node bin/create-list-of-icons.js",
    "download-css-and-fonts": "node bin/download-css-and-fonts.js",
    "download-all": "npm run download-icons && npm run download-css-and-fonts",
    "dev": "vite",
    "build": "npm run build:ts && npm run build:dts",
    "build:ts": "rimraf ./dist && rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose && tail -n +2 src/globalExtensions.ts >> dist/vue-material-icons.d.ts",
    "lint": "npm run lint:vue && npm run lint:css",
    "lint:vue": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.scss"
  },
  "main": "dist/vue-material-icons.js",
  "module": "dist/vue-material-icons.es.js",
  "types": "dist/vue-material-icons.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dbetka/utils": "^0.1.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.2.6",
    "async": "^3.2.0",
    "chalk": "^4.1.2",
    "download": "^8.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-vue": "^7.12.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^6.0.1",
    "replace-in-file": "^6.2.0",
    "rollup": "^2.52.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-sass": "^1.2.6",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.38.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vue": "^3.2.6"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.2.6",
    "vue": "^3.2.6"
  }
}
