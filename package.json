{
  "name": "@dbetka/vue-material-icons",
  "version": "0.3.4",
  "description": "Vue3 Material Design Icons",
  "license": "MIT",
  "author": "dbetka",
  "keywords": [
    "material",
    "design",
    "icons",
    "vue",
    "vue3",
    "dbetka",
    "@dbetka",
    "dbetka",
    "google"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dbetka/vue-material-icons"
  },
  "scripts": {
    "download-icons": "node bin/create-list-of-icons.js",
    "download-css-and-fonts": "node bin/download-css-and-fonts.js",
    "download-all": "npm run download-icons && npm run download-css-and-fonts",
    "dev": "vite",
    "download-and-build": "npm run download-all && npm run build",
    "build": "npm run build:ts && npm run build:dts",
    "build:ts": "rimraf ./dist && rollup -c rollup.config.mjs",
    "build:dts": "api-extractor run --local --verbose && tail -n +2 src/globalExtensions.ts >> dist/vue-material-icons.d.ts",
    "lint": "npm run lint:vue && npm run lint:sass",
    "lint:vue": "eslint src/**/*.vue",
    "lint:sass": "stylelint src/**/*.sass"
  },
  "main": "dist/vue-material-icons.js",
  "module": "dist/vue-material-icons.es.js",
  "types": "dist/vue-material-icons.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@microsoft/api-extractor": "^7.23.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.33",
    "async": "^3.2.3",
    "chalk": "^5.0.1",
    "download": "^8.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "node-fetch": "^2.6.7",
    "replace-in-file": "^6.3.2",
    "rollup": "^2.72.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-sass": "^1.2.12",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.2.33",
    "vue": "^3.2.6"
  }
}
